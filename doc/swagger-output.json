    "/login/": {
      "post": {
        "tags": [
          "Login"
        ],
        "summary": "Realiza o login e retorna um token JWT",
        "description": "",
        "responses": {
          "200": {
            "description": "Login realizado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "token": {
                    "type": "string",
                    "example": "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Credenciais inválidas"
          },
          "500": {
            "description": "Erro interno"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "usuario@exemplo.com"
                  },
                  "senha": {
                    "type": "string",
                    "example": "123456"
                  }
                },
                "required": [
                  "email",
                  "senha"
                ]
              }
            }
          }
        }
      }
    },
